        <ScrollPane fx:id="sp" prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0">
                    <children>
                        <ImageView fx:id="image" fitHeight="480.0" fitWidth="640.0" layoutX="10.0" layoutY="50.0" smooth="false" />
                    </children>
                </AnchorPane>
            </content>
        </ScrollPane>
        <Button fx:id="button" layoutX="10.0" layoutY="10.0" onAction="#handleButtonAction" text="Click Me!" />
        <Label fx:id="label" layoutX="10.0" layoutY="30.0" minHeight="16.0" minWidth="69.0" />
    </children>
</AnchorPane>
